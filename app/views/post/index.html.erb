<form action="/write" method="get">
  <div class="form-group">
    <label for="exampleInputEmail1">제목</label>
    <input name="title" type="text" class="form-control" id="exampleInputEmail1" aria-describedby="emailHelp" placeholder="제목을 적어주세요">
  </div>
  
  <div class="form-group">
    <label for="exampleTextarea">내용</label>
    <textarea name="content" placeholder="내용을 적어주세요." class="form-control" id="exampleTextarea" rows="3"></textarea>
  </div>
  
  <button type="submit" class="btn btn-primary">Submit</button>
</form>